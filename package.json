    "jest": "^26.6.3",
    "mock-apollo-client": "^1.0.0",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.5"
